SQL: INSERT INTO `lj_cheats` (`theme`,`content`,`datetime`,`game_id`,`game_list_small_id`,`game_list_large_id`,`user_id`) VALUES ('古墓丽影9邪马台宝藏获得技巧','<p>进去那个屋子后，先开窗子，然后跑到那个&ldquo;升降机&rdquo;开关前，等两三秒在开&ldquo;升降机&rdquo;要是对时间没什么概念的童鞋可以看那个窗子，总共有三扇窗子，等第一扇开了，就开升降机开关，然后马上跳上升降机，然后等着就行了，等风把升降机刮到最高点，对墙二段跳，就上去啦</p>\n','2014-02-23 18:47:24','28',2,1,6) [ RunTime:0.000317s ]

